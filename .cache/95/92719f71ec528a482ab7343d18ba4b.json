olute;\n  top: 0;\n  left: 0;\n  &.is--changed{\n    opacity: 1;\n    top: -240px;\n  }\n  *{\n    color: #fff;\n  }\n  p{\n    margin-bottom: 1rem;\n  }\n  .mail{\n    background-color: #f87474;\n    border-radius: 4px;\n    box-shadow: 1px 1px 2px 1px rgba(3,3,3,0.5);\n    color:#fff;\n    cursor: pointer;\n    display: block;\n    text-align: center;\n    margin: 1rem auto 0;\n    padding: 1rem;\n    width: 300px;\n  }\n}\n\n\n.c--section{\n  padding: 1rem;\n  width: 100%;\n  max-width: 900px;\n  margin: 0 auto;\n}\n.c--section-title{\n  font-size: 2rem;\n  max-width: 900px;\n  margin: 0;\n  width: 100%;\n}\n.c--section .skill-content{\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: center;\n  dl{\n    background-color: #FFF4E1;;\n    border: 3px solid #E67A7A;\n    border-radius: 3px;\n    width: 33%;\n    margin: 1rem;\n    padding: 1rem;\n  }\n}\n.section-content{\n\n}\n\n.c--section.product{\n  /*background-color: #FFF4E1;*/\n  width: 100%;\n}\n\n.c--experiment-list{\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n  width: 100%;\n  \n  li{\n    width: 100%;\n    @media screen and (min-width: 544px){\n      width: 47%;\n    }\n    @media screen and (min-width: 544px){\n      &:nth-child(1) .experiment-container{\n        margin: 0;\n      }\n      &:nth-child(2) .experiment-container{\n        margin: 60px 0 0;\n      }\n      &:nth-child(3) .experiment-container{\n        margin: 0;\n      }\n      &:nth-child(4) .experiment-container{\n        margin: 30px 0 0;\n      }\n      &:nth-child(5) .experiment-container{\n        margin: 0;\n      }\n    }\n    .experiment-container{\n      /*background-color: #fff;*/\n      background-color: rgb(33, 36, 45);\n      border-radius: 5px;\n      box-shadow: 0 20px 30px -15px rgba(0,0,0,0.08);\n      margin: 0 0 40px;\n      padding: 1rem;\n      width: 100%;\n      div{\n        border-bottom: 1px solid rgb(49,55,70);\n        color: #fff;\n        &:last-of-type{\n          border: